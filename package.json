{
  "name": "@seon/ui",
  "version": "5.100.7",
  "private": false,
  "files": [
    "dist",
    "apollo.js",
    "apollo.d.ts",
    "apps.js",
    "apps.d.ts",
    "context.js",
    "context.d.ts"
  ],
  "scripts": {
    "start": "webpack serve --port 3000",
    "start:development": "webpack serve --port 3000 --env development",
    "start:stage": "webpack serve --port 3000 --env stage",
    "start:prod": "webpack serve --port 3000 --env prod",
    "clean:dist": "rm -rf ./dist",
    "build:stage": "webpack --env stage",
    "build:prod": "webpack --env prod",
    "cdk:synth:stage": "cdk synth -c config=stage --all",
    "cdk:synth:prod": "cdk synth -c config=prod --all",
    "deploy:stage": "npm run clean:dist && npm run build:stage && cdk deploy -c config=stage --all",
    "deploy:prod": "npm run clean:dist && npm run build:prod && cdk deploy -c config=prod --all",
    "ui": "storybook dev -s ./src/static -p 9001",
    "clean": "rimraf coverage dist storybook-static",
    "build:storybook": "storybook build -s ./src/static port 9001",
    "build:files": "copyfiles -u 1 src/@types/* src/static/*/* dist",
    "build:package": "npm run clean && tsc && tsc-alias && npm run build:files",
    "prepublishOnly": "npm run build:package",
    "test": "jest src",
    "test:watch": "jest src --coverage=false --watchAll",
    "postpublish": "npm run clean",
    "codegen": "graphql-codegen --config codegen.yml",
    "tsc:check": "tsc --noEmit",
    "lint:prettier": "prettier . --fix",
    "lint:eslint": "eslint . --ext .ts,.tsx --fix",
    "lint:stylelint": "stylelint 'src/**/*.{ts,tsx}' --cache",
    "npm:scope": "npm config set registry https://registry.npmjs.com/",
    "aws:login": "aws codeartifact login --tool npm --repository seon-control-room --domain seon --domain-owner 894556524073 --region eu-central-1",
    "aws:publish": "npm run aws:login && npm publish",
    "ts:trace": "tsc --generateTrace ./outDir",
    "export-token": "export CODEARTIFACT_AUTH_TOKEN=`aws codeartifact get-authorization-token --domain seon --domain-owner 894556524073 --region eu-central-1 --query authorizationToken --output text`"
  },
  "browserslist": {
    "production": [
      "last 2 chrome version",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "not op_mini all"
    ]
  },
  "engines": {
    "node": "18.16.1"
  },
  "resolutions": {
    "typescript": "5.1.6"
  },
  "overrides": {
    "typescript": "5.1.6"
  },
  "dependencies": {
    "@apollo/client": "^3.7.5",
    "@nivo/bar": "^0.85.1",
