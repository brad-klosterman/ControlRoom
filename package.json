    "framer-motion": "^9.0.1",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.5.5",
    "iso-3166-ts": "^2.2.0",
    "luxon": "^3.4.3",
    "markdown-to-jsx": "^7.1.9",
    "moment": "^2.29.1",
    "polished": "^4.2.2",
    "qs": "^6.10.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-geocode": "^0.2.3",
    "react-hook-form": "^7.45.1",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-is": "^18.2.0",
    "react-player": "^2.11.0",
